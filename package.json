{
    "name": "n8n-nodes-haveibeenpwned",
    "version": "0.1.0",
    "description": "n8n community node for Have I Been Pwned - check if emails or passwords have been exposed in data breaches",
    "license": "MIT",
    "homepage": "https://github.com/pemontto/n8n-nodes-haveibeenpwned",
    "keywords": [
        "n8n-community-node-package",
        "haveibeenpwned",
        "hibp",
        "security",
        "breach",
        "password"
    ],
    "author": {
        "name": "pemontto"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/pemontto/n8n-nodes-haveibeenpwned.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/HaveIBeenPwnedApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/HaveIBeenPwned/HaveIBeenPwned.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/node": "^25.0.3",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
